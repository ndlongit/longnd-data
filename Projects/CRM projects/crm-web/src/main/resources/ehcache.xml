<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
  <diskStore path="java.io.tmpdir"/>

  <defaultCache
    maxElementsInMemory="10000"
    eternal="false"
    timeToIdleSeconds="120"
    timeToLiveSeconds="120"
    overflowToDisk="true" />

  <cache name="CspCache"
    maxElementsInMemory="3"
    eternal="false"
    timeToIdleSeconds="0"
    timeToLiveSeconds="0"
    diskPersistent="true"
    overflowToDisk="true" />
</ehcache>
